{
  "name": "mup-aws-beanstalk",
  "version": "0.6.3",
  "description": "Deploy apps to AWS Elastic Beanstalk using Meteor Up",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib --sourceRoot=./ --copy-files --sourcemaps",
    "build:watch": "npm run build -s -- -w",
    "lint": "npm run lint:code && npm run lint:docs",
    "lint:docs": "node ./tests/validate-docs.js",
    "lint:code": "eslint .",
    "pretest": "npm run lint -s",
    "test": "mocha --require babel-register --require babel-polyfill ./tests/*.spec.js"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "gfm-code-blocks": "^1.0.0",
    "mocha": "^7.0.0",
    "mup": "git+https://github.com/paulincai/meteor-up.git",
    "shelljs": "^0.8.3"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "aws-sdk": "^2.601.0",
    "axios": "^0.19.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^3.0.0",
    "ejs": "^3.0.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "random-seed": "^0.3.0",
    "shell-escape": "^0.2.0",
    "tar": "^5.0.5",
    "uuid": "^3.3.3"
  },
  "resolutions": {
    "**/**/node-gyp": "5.0.0"
  }
}
